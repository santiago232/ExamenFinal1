<Window x:Class="ExamenFinal1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamenFinal1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label>Carnet:</Label>
            <TextBox x:Name="txtCarnet" />
            <Label>Nombre:</Label>
            <TextBox x:Name="txtNombre" />
            <Label>Teléfono:</Label>
            <TextBox x:Name="txtTelefono" />
            <Label>Grado:</Label>
            <TextBox x:Name="txtGrado" />
            <Label>Usuario:</Label>
            <ComboBox x:Name="cmbUsuario" />
            <Button Content="Guardar" Click="Guardar_Click" />
        </StackPanel>


    </Grid>
</Window>
